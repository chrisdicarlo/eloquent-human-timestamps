    "require": {
        "php": "^8.2|^8.3|^8.4",
        "illuminate/support": "^11"
    },
    "require-dev": {
        "orchestra/testbench": "^9.0",
        "phpunit/phpunit": "^9.4|^9.5.4|^10"
    },
    "autoload": {
        "psr-4": {
            "ChrisDiCarlo\\EloquentHumanTimestamps\\": "src"
	    }
